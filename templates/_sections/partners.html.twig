{% set partners = [
    {
        "name": 'Яндекс Такси',
        "logo": asset('img/sec-partners/partners/yandex_taxi.jpg')
    },{
        "name": 'АО "Сельскохозяйственная Техника"',
        "logo": asset('img/sec-partners/partners/cktek.png')
    },{
        "name": 'АБ "Иванян и партнеры"',
        "logo": asset('img/sec-partners/partners/ivanyan.png')
    },{
        "name": 'АКБ "Мастре-Гард"',
        "logo": asset('img/sec-partners/partners/master-gard.png')
    },{
        "name": 'ООО "СуперМедик"',
        "logo": asset('img/sec-partners/partners/medic.png')
    },{
        "name": 'ГБУК МТ "Новая Опера"',
        "logo": asset('img/sec-partners/partners/noop_ru_rs.png')
    },{
        "name": 'ООО "Рустэк"',
        "logo": asset('img/sec-partners/partners/rusteck.png')
    },{
        "name": 'Спецстрой России',
        "logo": asset('img/sec-partners/partners/spec_stoy.png')
    },{
        "name": 'ООО "ВГЭС Проект"',
        "logo": asset('img/sec-partners/partners/vges.jpg')
    }
] %}

<section class="sec-partners">
    <div class="container">
        <h2 class="sec-partners__title">Наши корпоративные клиенты</h2>
        <div class="sec-partners__slider">
            <div class="swiper-container js-partners-swiper">
                <div class="swiper-wrapper">
                    {% for partner in partners %}
                        <div class="swiper-slide sec-partners__slide">
                            <img class="sec-partners__logo lazyload" data-src="{{ partner.logo }}" loading="lazy" alt="{{ partner.name }}">
                        </div>
                    {% endfor %}
                </div>
                <div class="swiper-pagination"></div>
            </div>
            <div class="slick-arrow slick-prev"></div>
            <div class="slick-arrow slick-next"></div>
        </div>
    </div>
</section>
