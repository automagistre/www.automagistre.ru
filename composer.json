{
    "name": "automagistre/www.automagistre.ru",
    "description": "Customer website of Automagistre LLC",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "sensio/framework-extra-bundle": "^5.3",
        "sensiolabs/security-checker": "^6.0",
        "sentry/sentry-symfony": "^3.0",
        "symfony/asset": "^5.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^5.0",
        "symfony/event-dispatcher": "^4.2",
        "symfony/expression-language": "^5.0",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-kernel": "^4.3.0",
        "symfony/mailer": "^5.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "^4.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/templating": "^5.0",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "brianium/paratest": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "pedrotroller/php-cs-custom-fixer": "^2.17",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-symfony": "^0.11.5",
        "phpunit/phpunit": "^8.1",
        "psalm/plugin-phpunit": "^0.8.1",
        "symfony/browser-kit": "^5.0",
        "symfony/debug-bundle": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/requirements-checker": "^1.1",
        "symfony/web-profiler-bundle": "^5.0",
        "vimeo/psalm": "^3.2"
    },
    "scripts": {
        "auto-scripts": [
            "bin/console cache:warmup",
            "bin/console assets:install public"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests"
    },
    "config": {
        "cache-dir": "/var/cache/composer",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
